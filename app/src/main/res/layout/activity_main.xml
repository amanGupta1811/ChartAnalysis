        android:src="@drawable/ic_baseline_call_24"
        app:layout_constraintBottom_toTopOf="@id/btn_contact"
        app:layout_constraintEnd_toEndOf="parent"/>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
